        <div class="w-max  mx-auto relative top-0 h-12 bg-slate-100">
            <% for (let i=0; i < attribute.length; i++) { %>
                <a href="/filters/<%= attribute[i].id %>" class="w-max relative px-3 top-2 text-gray-500 bg-slate-100 "><%= attribute[i].name %></a>
            <% } %>
        </div>
    </div>
</div>